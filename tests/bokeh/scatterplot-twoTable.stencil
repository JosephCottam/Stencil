(import BokehRuntime)
(import py-math)

(table dataset 
  (fields a b c)
  (data (init (let (x : (range 0 100 6))
                   (y : (map sin x))
                   (z : (map cos x))
                   (ptuples (fields a b c) x y z)))))
                   

(table plot
  (render scatter (bind auto))
  (data
    (pull dataset 
          (let (x : a)
               (y : b)
               (color : "RED")))))
