(import BokehRuntime)

(table dataset 
  (fields a b c)
  (data (init (let (x : (range 0 100 6))
                   (y : (map sin x)))
                   (z : (map cos x))
                   (ptuples (fields x y z) x y z)))))

(table plot
  (render scatter (bind auto))
  (data
    (pull dataset 
          (let (x : a)
               (y : b)
               (color : "RED")))))
