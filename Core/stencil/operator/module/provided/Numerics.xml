
<ModuleData grammarRev="2" name="Numerics">
	<Class name="stencil.operator.module.provided.Numerics"/>
	<Description><![CDATA[Math functions.]]></Description>

	<Defaults>
		<Opts>
			<Opt specializer="[n .. n]"/>
		</Opts>
		<Facets>
			<Facet name="map"   type="PROJECT" function="true" prototype="VALUE"/>
			<Facet name="query" type="PROJECT" function="true" prototype="VALUE"/>
		</Facets>
	</Defaults>

	<Operator name="Abs"/>
	<Operator name="Add"/>
	<Operator name="Add1"/>
	<Operator name="AsNumber"/>
	<Operator name="Ceil"/>
	<Operator name="Div"/>
	<Operator name="Pow"/>
	<Operator name="Divide"/>
	<Operator name="Floor"/>
	<Operator name="Log"/>
	<Operator name="Log10"/>
	<Operator name="Max"/>
	<Operator name="Min"/>
	<Operator name="Mod"/>
	<Operator name="Mult"/>
	<Operator name="Negate"/>
	<Operator name="Round"/>
	<Operator name="Nearest"/>
	<Operator name="Sqrt"/>
	<Operator name="Sub"/>
	<Operator name="Sub1"/>

	<Operator name="Sum" target="Add" >
		<Opt specializer="[1 .. n]"/>
		<Facet name="Map"  type="PROJECT" function="false" />
		<Facet name="Query" type="PROJECT" function="false" />
		<Facet name="Guide" function="false" type="PROJECT" prototype="VALUE"/>
	</Operator>

</ModuleData>