<ModuleData grammarRev="2" name="StencilUtil">
	<Class name="stencil.operator.module.provided.StencilUtil"/>
	<Description><![CDATA[Utilities to manipulate the tuple data representation.]]></Description>

	<Defaults>
		<Opts>
			<Opt specializer="[1 .. n]"/>
		</Opts>
		<Facets>
			<Facet name="map"  function="false" type="PROJECT" prototype="VALUE"/>
			<Facet name="query" function="false" type="PROJECT" prototype="VALUE"/>
			<Facet name="stateID" function="false" type="CATEGORIZE" prototype="VALUE"/>
		</Facets>
	</Defaults>

	<Operator name="EchoCategorize">
		<Facet name="map" function="false" type="CATEGORIZE" prototype="%"/>
		<Facet name="query" function="false" type="CATEGORIZE" prototype="%"/>
        <Facet name="stateID" function="false" type="CATEGORIZE" prototype="VALUE"/>
	</Operator>
	
	<Operator name="EchoContinuous">
		<Facet name="map" function="false" type="CATEGORIZE" prototype="%"/>
		<Facet name="query" function="false" type="CATEGORIZE" prototype="%"/>
        <Facet name="stateID" function="false" type="CATEGORIZE" prototype="VALUE"/>
	</Operator>

</ModuleData>