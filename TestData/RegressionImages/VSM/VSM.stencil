STREAM: File
NAME: VertexList
HEADER: ID1,ID2
SEPARATOR: ,
SOURCE: ./ws_400_12_0.1.adj
SKIP: 1
STRICT: true
STREAM: File
NAME: BFS
HEADER: ID
SEPARATOR: ,
SOURCE: ./ws_400_12_0.1.bfs
SKIP: 1
STRICT: true

stream VertexList(ID1, ID2)
stream BFS(ID)

order BFS > VertexList

stream VertexPair(ID1, ID2)
from VertexList
   (ID1, ID2): (ID1, ID2)
from VertexList
   (ID1, ID2): (ID2, ID1)

layer VSM
from VertexPair
   ID : Concatenate(ID1, ID2) 
   X : Ordering(ID1)
   Y : Ordering(ID2) -> Mult(_, -1)
   FILL_COLOR: @Color{RED}
   (SIZE, SHAPE) : (1, "RECTANGLE")
 
stream Prime()
from BFS
	() : Ordering(ID) -> ()

operator Ordering (ID) -> (VALUE)
	(ALL) => VALUE : Index(ID) -> VALUE

