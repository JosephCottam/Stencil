layer VSM
from VertexPair
   ID : Concatenate(ID1, ID2) 
   X : Ordering(ID1)
   Y : Ordering(ID2)
   FILL_COLOR: Color{RED}
   (SIZE, SHAPE) : (1, "RECTANGLE")
 
stream Prime()
from BFS
	() : Ordering(ID) -> ()

operator Ordering (ID) -> (VALUE)
	default => VALUE : Index(ID)

