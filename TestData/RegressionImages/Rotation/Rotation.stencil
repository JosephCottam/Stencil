STREAM: File
NAME: Elements
HEADER: Name
SEPARATOR: ,
SOURCE: ./RotationData.txt
STRICT: TRUE
SKIP: 1


stream Elements(Name)

layer Text["TEXT"]
from Elements
	ID : Name
	TEXT: Name
	ROTATION: Count(Name) -> Mult(12.5, VALUE)
	X : Count(Name) -> Mult(40, VALUE)

layer Shape
from Elements
	ID: Concatenate(Name, "TL")
	SHAPE: "CROSS"
	ROTATION: Count(Name) -> Mult(12.5, VALUE)
	X : Count(Name) -> Mult(40, VALUE)
	Y : 40
	SIZE: 30
 	FILL_COLOR: @Color{RED}

from Elements
	ID: Concatenate(Name, "Center")
	SHAPE: "CROSS"
	REGISTRATION: "CENTER"
	ROTATION: Count(Name) -> Mult(12.8, VALUE) 
	X : Count(Name) -> Mult(40, _) -> Add(15,_)
	Y : 100
	SIZE: 30
 	FILL_COLOR: @Color{BLUE}



operator Count(Name) -> (VALUE) 
	(ALL) => VALUE : Index(Name)
