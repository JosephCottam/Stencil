stream Elements(Name) : Text["./RotationData.txt"]

layer Text["TEXT"]
from Elements
    ID : Name
    TEXT: Name
    ROTATION: Count(Name) -> Mult(12.5, Count) -> ToRadians(_)
    X : Count(Name) -> Mult(40, Count)

layer TopLeft
from Elements
    ID: Concatenate(Name, "TL")
    SHAPE: "CROSS"
    ROTATION: Count(Name) -> Mult(12.5, Count) -> ToRadians(_)
    X : Count(Name) -> Mult(40, Count) 
    Y : 40
    SIZE: 30
    FILL_COLOR: Color{RED}

layer Center
from Elements
    ID: Concatenate(Name, "Center")
    SHAPE: "CROSS"
    REGISTRATION: "CENTER"
    ROTATION: Count(Name) -> Mult(12.8, Count) -> ToRadians(_)
    X : Count(Name) -> Mult(40, _) -> Add(15,_)
    Y : 100
    SIZE: 30
    FILL_COLOR: Color{BLUE}



operator Count(Name) -> (VALUE) 
    default => VALUE : Index(Name)
