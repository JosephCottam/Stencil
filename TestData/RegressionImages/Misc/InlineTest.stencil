STREAM: File
NAME: Elements
HEADER: Name
SEPARATOR: ,
SOURCE: ../Rotation/RotationData.txt
SKIP: 1
STRICT: true


stream Elements(Name)

layer Shape
from Elements
	ID: Concatenate(Name, "TL")
	SHAPE: "CROSS"
	X : XLayout(Name) -> Get(*, 0) -> Mult(40, _)
   FILL_COLOR: @Color{RED,128}

layer Shape
from Elements
	ID: Concatenate(Name, "TL")
	SHAPE: "CROSS"
	X : XLayout(Name) -> Mult(40, VALUE)
   FILL_COLOR: @Color{GREEN, 128}

operator XLayout(Name) -> (VALUE) 
	(ALL) => VALUE : Index(Name)
