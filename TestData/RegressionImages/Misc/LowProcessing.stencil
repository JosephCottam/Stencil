/**Test with very little analysis overhead.
   Stresses the parsing and glyph creation system.
 **/
 
stream NodePositions(ID, X,Y) : Text["../Sourceforge/vx_cluster_0_8.6_min_cuts.coord", sep:"\\s+", skip:0] /*Precomputed layout*/
stream Attributes(ID, ATT) : Text["../Sourceforge/project_troves.txt", sep:"\\|", skip:1]       /*Additional attributes*/

order NodePositions > Attributes

layer Overlay
from NodePositions
	ID : Counter()
	(X,Y) : Counter() -> (_,_)
from Attributes
	ID : Counter()
	(X,Y) : Counter() -> (_,_)

operator Counter : Count
