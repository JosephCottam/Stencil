STREAM: File
NAME: survey
TUPLE_SIZE: 1
SEPARATOR: $^
SOURCE: ./Fruits.txt
SKIP: 0
STRICT: true


stream survey(fruit)

layer plot
guide
  axis from X
from survey
   ID: Count()
   X:* Index(fruit) -> Mult(5, Index)
   Y: Count(fruit) -> Mult(5, Count)
   REGISTRATION: "CENTER"
   FILL_COLOR: Color{150,150,255}
   