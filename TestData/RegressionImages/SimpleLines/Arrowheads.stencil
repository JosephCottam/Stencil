STREAM: File
NAME: Degrees
HEADER: Deg
SEPARATOR: ,
SOURCE: ./degrees.txt
SKIP: 1
STRICT: true

stream Degrees(Deg)

layer DoubleLines[LINE]
default
  CAP1: "ARROW"
  CAP2: "ARROW"
from Degrees
  ID  : IDS("")
  X.1 : Cosine(Deg) -> Mult(_,-50)
  Y.1 : Sine(Deg) -> Mult(_,-50)
  X.2 : Cosine(Deg) -> Mult(_,50)
  Y.2 : Sine(Deg) -> Mult(_,50)
from Degrees
  ID  : IDS("")
  X.1 : Add(90, Deg) -> Cosine(_) -> Mult(_,-50)
  Y.1 : Add(90, Deg) -> Sine(_) -> Mult(_,-50)
  X.2 : Add(90, Deg) -> Cosine(_) -> Mult(_,50)
  Y.2 : Add(90, Deg) -> Sine(_) -> Mult(_,50)
  STROKE_COLOR: @Color{GRAY70}


