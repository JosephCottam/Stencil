STREAM: File
NAME: Renders
TUPLE_SIZE: 2
SEPARATOR: \s*,\s*
SOURCE: /Volumes/scratch/jcottam/Documents/Papers/others/dissertation/figures/stencilExamples/ScheduleOpt/RenderCounts.csv
SKIP: 1
STRICT: true

stream Renders(Config, Count)

layer Chart
  guide axis from X
  		  pointLabels from ID
           (X,Y) : Chart.find(ID) -> Reg["TOP"](Chart.*)
           REGISTRATION : "BOTTOM"
           FONT: @Font{6}
			  COLOR : @Color{GRAY50}
from Renders
  ID: Count
  X:* Rank(Config) -> Mult(10, _)
  Y: 0
  SHAPE: "RECTANGLE"
  HEIGHT: Mult(Count, 5)
  REGISTRATION: "BOTTOM"

