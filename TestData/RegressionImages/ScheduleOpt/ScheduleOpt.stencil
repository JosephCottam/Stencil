stream Renders(Config, Count) : Text["./RenderCounts.csv"]

layer Chart
  guide axis from X
    pointLabels from ID
    (X,Y) : Chart.find(ID) -> Reg["TOP"](Chart.*)
    REGISTRATION : "BOTTOM"
    FONT: Font{6}
    COLOR : Color{GRAY50}
from Renders
  ID: Count
  X:* Rank(Config) -> Mult(10, _)
  Y: 0
  SHAPE: "RECTANGLE"
  HEIGHT: Mult(Count, 5)
  REGISTRATION: "BOTTOM"

