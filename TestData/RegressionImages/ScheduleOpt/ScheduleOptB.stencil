STREAM: File
NAME: Renders
TUPLE_SIZE: 2
SEPARATOR: \s*,\s*
SOURCE: /Volumes/scratch/jcottam/Documents/Papers/others/dissertation/figures/stencilExamples/ScheduleOpt/RenderCounts.csv
SKIP: 1
STRICT: true

stream Renders(Config, Count)

layer Chart
  guide axis from X
        axis[stride:2] from Y
from Renders
  ID: Config
  X:* Rank(Config) -> Mult(10, _)
  Y: Mult(Count, 5)
  SHAPE: "RECTANGLE"
  HEIGHT: Mult(Count, 5)
  REGISTRATION: "TOP"

